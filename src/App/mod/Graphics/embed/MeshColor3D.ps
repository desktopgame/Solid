struct Output {
    float4 svpos : SV_POSITION;
    float4 color : COLOR;
};
struct PSOutput
{
    float4 outPosition : SV_Target0;
    float4 outNormal : SV_Target1;
    float4 outColor : SV_Target2;
};

PSOutput psMain(Output input) : SV_TARGET {
    PSOutput output;
    output.outColor = input.color;
    return output;
}